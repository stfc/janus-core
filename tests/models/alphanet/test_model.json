{
  "data": {
    "root": "dataset/",
    "dataset_name": "t1",
    "target": "energy_force",
    "train_dataset": "aqcat",
    "valid_dataset": "test_ood_ads",
    "test_dataset": "test_ood_mat",
    "seed": 42
  },
  "model": {
    "name": "Alphanet",
    "num_layers": 4,
    "num_targets": 1,
    "output_dim": 1,
    "compute_forces": true,
    "compute_stress": false,
    "main_chi1":  64,
    "mp_chi1":  48,
    "chi2": 32,
    "use_pbc": true,
    "has_dropout_flag": false,
    "hidden_channels": 128,
    "cutoff": 6,
    "num_radial": 8
  },
  "train": {
    "epochs": 500,
    "batch_size":32,
    "accumulation_steps": 1,
    "vt_batch_size": 32,
    "lr": 0.0002,
    "lr_decay_factor": 0.9,
    "lr_decay_step_size": 50000,
    "weight_decay": 0.000,
    "save_dir": "./dac",
    "log_dir": "",
    "disable_tqdm": false,
    "scheduler": "steplr",
    "device": "cuda",
    "energy_loss": "mae",
    "force_loss": "mae",
    "stress_loss": "mae",
    "energy_metric": "mae",
    "force_metric": "mae",
    "energy_coef": 4.0,
    "stress_coef": 0.5,
    "force_coef": 100.0,
    "eval_steps": 1
  }
}
